#!/bin/bash

declare -a acceptors=( "-ioe" "-oi" "-sa" "-ailta")
declare -a selectors=("-rd" "-rp" "-rpd" "-sr")

for acc in "${acceptors[@]}"
do
  for sel in "${selectors[@]}"
  do
    for order in 4 #4 5
    do
      for p in `seq 0 5 100`
      do
        for i in {1..5} #{1..20}
        do
          echo "benchmark_puzzles/benchmarks${order}x${order}/${p}/puzzle${i}.txt"
          ./cmake-build-release/sudoku_oop.exe ./cmake-build-release/benchmark_puzzles/benchmarks${order}x${order}/"${p}"/puzzle${i}.txt output.txt answer.txt "$acc" "$sel"
        done
      done
    done
  done
done